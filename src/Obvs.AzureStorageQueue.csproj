<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <NoWarn>$(NoWarn);1591</NoWarn>
        <Authors>Fernando Almeida</Authors>
        <Copyright>Copyright Â©2018</Copyright>
        <Product>Obvs.Microsoft.Azure.Queue</Product>
        <Description>Obvs transport for Windows Azure Storage queues</Description>
        <GenerateFullPaths>true</GenerateFullPaths>
        <Version>1.0.0</Version>
        <GenerateFullPaths>true</GenerateFullPaths>
        <NeutralLanguage>en</NeutralLanguage>
        <RepositoryUrl>https://github.com/fernando-almeida/Obvs.AzureStorageQueue.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Storage.Queue" Version="9.4.2"/>
    <PackageReference Include="Obvs" Version="5.0.0.17"/>
  </ItemGroup>
    <ItemGroup>
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
        <_Parameter1>$(MSBuildProjectName).Tests</_Parameter1>
      </AssemblyAttribute>
  </ItemGroup>
</Project>
